using System;

class Program
{
    static void Main()
    {
        int number, sum = 0;

        do
        {
            Console.Write("Enter a positive number: ");
            number = Convert.ToInt32(Console.ReadLine());
            if (number > 0)
                sum += number;
        } while (number > 0);

        Console.WriteLine("The sum of the positive numbers is: " + sum);
    }
}
